    <section id="report-page-0" class="reporting-page">
      <div class="reporting-page-content">
        <header class="reporting-cover-page-header">
          <img
            class="reporting-page-header-logo"
            src="{#asset ./assets/img-your-logo-here.png @encoding=dataURI}"
            alt="Logo"
          />
        </header>

        <main>
          <article class="reporting-cover-page-title">
            <div class="reporting-cover-page-info">
              <div class="reporting-cover-page-title-info">
                <h1 class="reporting-cover-page-title-name">
                  Dark Web <br />Monthly Report
                </h1>
              </div>

              <div class="reporting-cover-page-client-info">
                <span class="reporting-cover-page-client-name"
                  >{{clientName}}</span
                >
                <span class="reporting-cover-page-client-date"
                  >{{getMonthName breach_month}} {{dateNow}}, {{breach_year}}</span
